{
  "name": "angular-env",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "NODE_ENV=development ng serve --configuration development",
    "start:test": "NODE_ENV=test ng serve --configuration test",
    "start:staging": "NODE_ENV=staging ng serve --configuration staging",
    "serve:prod": "NODE_ENV=production ng serve --configuration production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "load-env:dev": "npx ts-node -r esm -r dotenv/config src/environments/environment.ts src/environments/dotenv.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "assert": "^2.0.0",
    "constants-browserify": "^1.0.0",
    "file-system": "^2.2.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "graceful-fs": "^4.2.9",
    "os-browserify": "^0.3.0",
    "path": "^0.12.7",
    "promise-fs": "^2.1.1",
    "rxjs": "~7.4.0",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@ngx-env/builder": "^2.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.46",
    "babel-plugin-dotenv": "^0.1.1",
    "dotenv": "^16.0.0",
    "esm": "^3.2.25",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}